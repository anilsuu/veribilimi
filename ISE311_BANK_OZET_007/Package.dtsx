                // Unicode karakter kodları ile garanti dönüşüm
                duzenlenmisAd = duzenlenmisAd
                    .Replace("Ğ", "G").Replace("\u011E", "G")
                    .Replace("Ü", "U").Replace("\u00DC", "U")
                    .Replace("Ş", "S").Replace("\u015E", "S")
                    .Replace("İ", "I").Replace("\u0130", "I")
                    .Replace("Ö", "O").Replace("\u00D6", "O")
                    .Replace("Ç", "C").Replace("\u00C7", "C");

                // 3. Tam Yolu Oluştur (Örn: ...\1_MB\ANKARA)
                string tamYol = Path.Combine(bolgeYolu, duzenlenmisAd);

                // 4. Değişkene Ata
                Dts.Variables["User::SehirKlasorYolu"].Value = tamYol;

                // ---------------------------------------------------------
                // ⭐ SİHİRLİ DOKUNUŞ: KLASÖRÜ FİZİKSEL OLARAK OLUŞTUR
                // ---------------------------------------------------------
                if (!Directory.Exists(tamYol))
                {
                    Directory.CreateDirectory(tamYol);
                }

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                MessageBox.Show("Şehir Klasörü Hatası: " + ex.Message);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

        #region ScriptResults declaration
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion
    }
}]]></ProjectItem>
