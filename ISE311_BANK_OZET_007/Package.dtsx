        public void Main()
        {
            try
            {
                string anaYol = Dts.Variables["User::AnaKlasorYolu"].Value.ToString();
                string bolgeAd = Dts.Variables["User::BolgeAd"].Value.ToString();
                string bolgeID = Dts.Variables["User::BolgeID"].Value.ToString();

                // 1. Kısaltma Mantığı (Kelimelerin baş harflerini al)
                string[] kelimeler = bolgeAd.Split(' ');
                string kisaltma = "";
                foreach (string k in kelimeler) { if (k.Length > 0) kisaltma += k[0]; }

                // 2. Türkçe Karakterleri Temizle
                kisaltma = kisaltma.ToUpper()
                    .Replace("Ğ", "G").Replace("\u011E", "G")
                    .Replace("Ü", "U").Replace("\u00DC", "U")
                    .Replace("Ş", "S").Replace("\u015E", "S")
                    .Replace("İ", "I").Replace("\u0130", "I")
                    .Replace("Ö", "O").Replace("\u00D6", "O")
                    .Replace("Ç", "C").Replace("\u00C7", "C");

                // ⭐ 3. İSTİSNA KONTROLÜ: IB -> IAB, GB -> GAB, DB -> DAB Dönüşümü
                // Eğer kisaltma sadece "IB" ise ve isimde "Anadolu" geçiyorsa IAB yap
                if (kisaltma == "IB" && bolgeAd.ToUpper().Contains("ANADOLU"))
                {
                    kisaltma = "IA";
                }
                else if (kisaltma == "GB" && bolgeAd.ToUpper().Contains("GÜNEY"))
                {
                    kisaltma = "GA";
                }
                else if (kisaltma == "DB" && bolgeAd.ToUpper().Contains("DOĞU"))
                {
                    kisaltma = "DA";
                }

                // Sonuna "B" (Bölge) harfini ekle
                string tamKisaltma = kisaltma + "B";
                string klasorAdi = bolgeID + "_" + tamKisaltma;

                // 4. Tam Yolu Hesapla ve Değişkene Ata
                string tamYol = System.IO.Path.Combine(anaYol, klasorAdi);
                Dts.Variables["User::BolgeKlasorYolu"].Value = tamYol;
             
                // Klasörü fiziksel olarak oluştur
                if (!System.IO.Directory.Exists(tamYol))
                {
                    System.IO.Directory.CreateDirectory(tamYol);
                }
               // Script'in sonunda değişkeni güncelle
                Dts.Variables["User::BolgeKlasorYolu"].Value = tamYol;
                
                Dts.TaskResult = (int)ScriptResults.Success;
                // Kontrol için mesaj kutusu kalsın (İstersen sonra silersin)
                MessageBox.Show("Klasör Oluşturuldu: " + tamYol);
            }
            catch (Exception ex)
            {
                MessageBox.Show("Hata: " + ex.Message);
                //        Dts.TaskResult = (int)ScriptResults.Failure;
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

        #region ScriptResults declaration
        enum ScriptResults
            {
                Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
                Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
