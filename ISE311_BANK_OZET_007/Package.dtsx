    {
        public void Main()
        {
            try
            {
                // 1. Değişkenleri al
                // Bölge klasör yolu zaten dışarıda oluşmuştu (Örn: C:\ISE311P02007\1_MB)
                string bolgeYolu = Dts.Variables["User::BolgeKlasorYolu"].Value.ToString();
                string hamAd = Dts.Variables["User::SehirAd"].Value.ToString();

                // 2. Şehir Adını Düzenle (Türkçe -> İngilizce)
